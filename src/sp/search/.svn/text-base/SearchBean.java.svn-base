package sp.search;

public class SearchBean {
    
    // 게시판 검색관련
    private String ct_id;       // 게시판 ID
    private String board_seq;   // 게시판 SEQ
    private String subject;     // 제목
    private String content;     // 내용
        
    // 웹검색 관련
    private String menu_id;     // 메뉴 ID
    private String m_menu;      // 대메뉴
    private String l_menu;      // 중메뉴
    private String s_munu;      // 소메뉴   
    private String title;       // 타이틀
    private String body;        // 내용
    
    public String getCt_id() {
        return ct_id;
    }
    
    public void setCt_id(String ct_id) {
        this.ct_id = ct_id;
    }
    
    public String getBoard_seq() {
        return board_seq;
    }
    
    public void setBoard_seq(String board_seq) {
        this.board_seq = board_seq;
    }
    
    public String getSubject() {
        return subject;
    }
    
    public void setSubject(String subject) {
        this.subject = subject;
    }
    
    public String getContent() {
        return content;
    }
    
    public void setContent(String content) {
        this.content = content;
    }
    
    public String getMenu_id() {
        return menu_id;
    }
    
    public void setMenu_id(String menu_id) {
        this.menu_id = menu_id;
    }
    
    public String getM_menu() {
        return m_menu;
    }
    
    public void setM_menu(String m_menu) {
        this.m_menu = m_menu;
    }
    
    public String getL_menu() {
        return l_menu;
    }
    
    public void setL_menu(String l_menu) {
        this.l_menu = l_menu;
    }
    
    public String getS_munu() {
        return s_munu;
    }
    
    public void setS_munu(String s_munu) {
        this.s_munu = s_munu;
    }
    
    public String getTitle() {
        return title;
    }
    
    public void setTitle(String title) {
        this.title = title;
    }

    public String getBody() {
        return body;
    }

    public void setBody(String body) {
        this.body = body;
    }    
    
}
